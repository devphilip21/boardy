{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc": "off",
        "indent": ["error", 2],
        "no-unused-vars": "off",
        "valid-jsdoc": "off",
        "no-invalid-this": "off",
        "max-len": ["error", { "code": 100 }],
        "guard-for-in": "off"
    }
}
